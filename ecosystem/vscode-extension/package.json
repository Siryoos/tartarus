{
    "name": "tartarus-vscode",
    "displayName": "Tartarus",
    "description": "VS Code extension for Tartarus",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:tartarus.helloWorld"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "tartarus.helloWorld",
                "title": "Hello Tartarus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "typescript": "^4.0.0"
    }
}